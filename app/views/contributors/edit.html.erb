<h2>プロフィール編集</h2>
<%= form_with model: @contributor, local:true do |f| %>

  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%><br>
  
  <label for="inputGender">Gender</label>
  <%= f.text_field :gender, autofocus: true, id:"inputGender", placeholder:"性別"%><br>
  
  <label for="inputAge">Age</label>
  <%= f.text_field :age, autofocus: true, id:"inputAge", placeholder:"年代"%><br>
  
  <label for="inputIntroduction">Introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"inputIntroduction", placeholder:"自己紹介"%><br>
　
  <%= f.submit "Update" %>
  
<% end %>