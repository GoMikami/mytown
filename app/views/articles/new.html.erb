<div class="new-article">  
  <h1>New Article</h1>
  <div class="form">
  <!-- form_with部分 -->
  <%= form_with model:@article, local:true do |f| %>
    <h4>画像</h4>
      <%= f.attachment_field :image %>
  　<h4>都道府県</h4>
      <%= f.text_field :prefecture_code %>
    <h4>市町村</h4>
      <%= f.text_field :address_city %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>本文</h4>
      <%= f.text_area :body %></br>
    <!--投稿ボタン-->
      <%= f.submit '投稿' %>
  <% end %>
  </div>
</div>